{
  "name": "@phone-stack/vps",
  "version": "1.0.0",
  "type": "module",
  "description": "Phone Stack VPS - Server provisioning, security, and management",
  "repository": {
    "type": "git",
    "url": "https://github.com/joeetdev/phone-stack-vps.git"
  },
  "license": "MIT",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./version": {
      "types": "./dist/version.d.ts",
      "default": "./dist/version.js"
    },
    "./ai": {
      "types": "./dist/configs/ai/index.d.ts",
      "default": "./dist/configs/ai/index.js"
    },
    "./configs": {
      "types": "./dist/configs/index.d.ts",
      "default": "./dist/configs/index.js"
    },
    "./scripts": {
      "types": "./dist/scripts/index.d.ts",
      "default": "./dist/scripts/index.js"
    },
    "./services": {
      "types": "./dist/services/index.d.ts",
      "default": "./dist/services/index.js"
    },
    "./services/enforcer": {
      "types": "./dist/services/enforcer/bundle.d.ts",
      "default": "./dist/services/enforcer/bundle.js"
    },
    "./services/file-api": {
      "types": "./dist/services/file-api/bundle.d.ts",
      "default": "./dist/services/file-api/bundle.js"
    },
    "./services/agent-bridge": {
      "types": "./dist/services/agent-bridge/bundle.d.ts",
      "default": "./dist/services/agent-bridge/bundle.js"
    },
    "./services/sovereign-shield": {
      "types": "./dist/services/sovereign-shield/bundle.d.ts",
      "default": "./dist/services/sovereign-shield/bundle.js"
    },
    "./services/caddy-gen": {
      "types": "./dist/services/caddy-gen/bundle.d.ts",
      "default": "./dist/services/caddy-gen/bundle.js"
    },
    "./services/watchdog": {
      "types": "./dist/services/watchdog/index.d.ts",
      "default": "./dist/services/watchdog/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "esbuild": "^0.24.2"
  },
  "devDependencies": {
    "@hono/node-server": "^1.13.7",
    "@simplewebauthn/browser": "^11.0.0",
    "@simplewebauthn/server": "^11.0.0",
    "@types/better-sqlite3": "^7.6.5",
    "@types/node": "^22.10.5",
    "@types/ws": "^8.5.13",
    "better-sqlite3": "^11.0.0",
    "hono": "^4.6.14",
    "tsup": "^8.0.0",
    "typescript": "^5.7.2",
    "ws": "^8.18.0"
  }
}
